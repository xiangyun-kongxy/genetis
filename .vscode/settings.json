    "explorer.autoReveal": false,

    "editor.fontFamily": "Source Code Pro",
    "editor.fontWeight": "600",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "editor.minimap.maxColumn": 80,
    "editor.wrappingIndent": "none",
    "editor.renderIndentGuides": true,
    "editor.hover.delay": 30,
    "clang.diagnostic.delay": 50,
    "clang.cxxflags": [
        "-fsyntax-only",
        "-std=c++11",
        "-I/usr/local/include/wx-3.1",
        "-I/usr/local/lib/wx/include/osx_cocoa-unicode-3.1",
        "-I${workspaceRoot}/src",
        "-I${workspaceRoot}/src/include",
        "-I${workspaceRoot}/src/util",
        "-I${workspaceRoot}/src/app/spl",
        "-I${workspaceRoot}/src/app/mind",
        "-I${workspaceRoot}/src/app/visual_plus",
        "-D_FILE_OFFSET_BITS", 
        "-DWXUSINGDLL", 
        "-D__WXMAC__", 
        "-D__WXOSX__", 
        "-D__WXOSX_COCOA__",
        "-DwxUSE_CONSTRAINTS",
        "-Wall"
        ],
    "clang.cflags": ["-fsyntax-only",
        "-I/usr/local/include/wx-3.1",
        "-I/usr/local/lib/wx/include/osx_cocoa-unicode-3.1",
